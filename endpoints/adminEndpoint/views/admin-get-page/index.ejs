												<i class="fa-solid fa-circle-check"></i>
											</div>
										</div>
									</td>

									<!--category-->
									<td class="--hidden">
										<div cell-container class="cell-container">
											<div
												class="cell-container__input-container cell-container__select-container"
											>
												<select product-category product-categories-select>
													<template>
														<option></option>
													</template>
												</select>
											</div>
											<div
												cell-info
												class="cell-container__cell-info-container"
											>
												<i
													class="cell-container__cell-info-container__loading"
												></i>
												<i
													cell-info-error
													class="fa-sharp fa-solid fa-circle-exclamation"
												></i>
												<i class="fa-solid fa-circle-check"></i>
											</div>
										</div>
									</td>

									<!--name-->
									<td>
										<div cell-container class="cell-container">
											<div
												class="cell-container__input-container cell-container__text-input-container"
											>
												<div
													product-name
													pseudo-input
													contenteditable="true"
													placeholder="Name"
													maxlength="30"
												></div>
											</div>
											<div
												cell-info
												class="cell-container__cell-info-container"
											>
												<i
													class="cell-container__cell-info-container__loading"
												></i>
												<i
													cell-info-error
													class="fa-sharp fa-solid fa-circle-exclamation"
												></i>
												<i class="fa-solid fa-circle-check"></i>
											</div>
										</div>
									</td>

									<!--price-->
									<td>
										<div cell-container class="cell-container">
											<div
												class="cell-container__input-container cell-container__text-input-container"
											>
												<div
													product-price
													pseudo-input
													contenteditable="true"
													placeholder="Price"
													maxlength="25"
												></div>
											</div>
											<div
												cell-info
												class="cell-container__cell-info-container"
											>
												<i
													class="cell-container__cell-info-container__loading"
												></i>
												<i
													cell-info-error
													class="fa-sharp fa-solid fa-circle-exclamation"
												></i>
												<i class="fa-solid fa-circle-check"></i>
											</div>
										</div>
									</td>

									<!--off-->
									<td>
										<div cell-container class="cell-container">
											<div
												class="cell-container__input-container cell-container__text-input-container"
											>
												<div
													product-off
													pseudo-input
													contenteditable="true"
													placeholder="Off"
													maxlength="30"
												></div>
											</div>
											<div
												cell-info
												class="cell-container__cell-info-container"
											>
												<i
													class="cell-container__cell-info-container__loading"
												></i>
												<i
													cell-info-error
													class="fa-sharp fa-solid fa-circle-exclamation"
												></i>
												<i class="fa-solid fa-circle-check"></i>
											</div>
										</div>
									</td>

									<!--installment-->
									<td class="--hidden">
										<div cell-container class="cell-container">
											<div
												class="cell-container__input-container cell-container__text-input-container"
											>
												<div
													product-installment
													pseudo-input
													contenteditable="true"
													placeholder="Installment"
													maxlength="30"
												></div>
											</div>
											<div
												cell-info
												class="cell-container__cell-info-container"
											>
												<i
													class="cell-container__cell-info-container__loading"
												></i>
												<i
													cell-info-error
													class="fa-sharp fa-solid fa-circle-exclamation"
												></i>
												<i class="fa-solid fa-circle-check"></i>
											</div>
										</div>
									</td>

									<!--whatsapp-->
									<td class="--hidden">
										<div cell-container class="cell-container">
											<div
												class="cell-container__input-container cell-container__text-input-container"
											>
												<div
													product-whatsapp
													pseudo-input
													contenteditable="true"
													placeholder="Whatsapp"
													maxlength="20"
